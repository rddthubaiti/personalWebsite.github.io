ter;\r\n  margin: auto;\r\n}\r\n\r\n.education-information li {\r\n  margin: auto;\r\n}\r\n\r\n.education-information li::marker {\r\n  content: \"â–º\";\r\n  color: #ffee32;\r\n}\r\n\r\n.title h1 {\r\n  font-size: 70px;\r\n  margin-bottom: 3vh;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n  text-decoration: underline;\r\n  text-decoration-color: #ffee32;\r\n  text-decoration-thickness: 5px;\r\n}\r\n\r\n.contact-from-items {\r\n  width: 100%;\r\n  position: relative;\r\n}\r\n\r\n.input {\r\n  width: 100%;\r\n  margin-bottom: 12px;\r\n  padding: 15px 15px;\r\n  box-sizing: border-box;\r\n  background: #ebeef1;\r\n  border: 0px;\r\n  outline: none;\r\n  border-radius: 30px;\r\n}\r\n\r\n.input-message {\r\n  height: 125px;\r\n  border-radius: 15px;\r\n  margin-bottom: 30px;\r\n  padding: 15px 15px;\r\n  resize: none;\r\n  background: #ebeef1;\r\n  outline: none;\r\n  border: 0;\r\n  overflow-y: hidden;\r\n}\r\n\r\n.contact-presentation .btn-primary,\r\n.contact-presentation .btn-primary:hover,\r\n.contact-presentation .btn-primary:active,\r\n.contact-presentation .btn-primary:visited,\r\n.contact-presentation .btn-primary:focus {\r\n  background-color: #ffee32;\r\n  border-color: #ffee32;\r\n  color: black;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n  padding: 12px;\r\n  border-radius: 25px;\r\n  border-style: none;\r\n  outline: none !important;\r\n  cursor: pointer;\r\n  width: 100%;\r\n}\r\n\r\n.perspective-text {\r\n  color: black;\r\n  font-family: \"Roboto\", sans-serif;\r\n  font-size: 58px;\r\n  font-weight: 900;\r\n  letter-spacing: -2px;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.perspective-line {\r\n  height: 50px;\r\n  overflow: hidden;\r\n  position: relative;\r\n}\r\n\r\n.sliding-perspective-content p {\r\n  margin: 0;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  transition: all 0.5s ease-in-out;\r\n}\r\n\r\n.perspective-line:nth-child(odd) {\r\n  transform: skew(60deg, -30deg) scaleY(0.667);\r\n}\r\n\r\n.perspective-